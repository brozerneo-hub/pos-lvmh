{
  "name": "@pos-lvmh/shared",
  "version": "0.1.0",
  "private": true,
  "description": "Types, schemas and constants shared between frontend and backend",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
